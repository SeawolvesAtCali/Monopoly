{
    "name": "@prisel/tic-tac-toe-client",
    "version": "0.1.3",
    "private": true,
    "description": "Tic tac toe client",
    "author": "Yiou Chen <chen.yiou1992@gmail.com>",
    "homepage": "https://github.com/SeawolvesAtCali/prisel/tree/master/packages/tic-tac-toe-client#readme",
    "license": "MIT",
    "main": "./lib/index.js",
    "directories": {
        "lib": "./",
        "test": "__test__"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "rules": {
            "react/prop-types": 0,
            "no-console": 0,
            "import/prefer-default-export": 0,
            "no-unused-vars": 0
        },
        "env": {
            "browser": true
        },
        "plugins": [
            "react"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SeawolvesAtCali/prisel.git"
    },
    "scripts": {
        "start": "nps",
        "build": "nps clean build",
        "test": "jest",
        "fix": "tslint --project tsconfig.json --fix",
        "storybook": "start-storybook -p 9001 -c .storybook"
    },
    "bugs": {
        "url": "https://github.com/SeawolvesAtCali/prisel/issues"
    },
    "dependencies": {
        "@prisel/client": "0.1.3",
        "normalize.css": "^8.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "cssnano": "^4.1.10",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/runtime": "^7.10.2",
        "regenerator-runtime": "^0.13.5",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@storybook/addon-actions": "^4.1.18",
        "@storybook/react": "^4.1.18",
        "babel-loader": "^8.1.0",
        "eslint-plugin-react": "^7.19.0",
        "parcel-bundler": "^1.12.4",
        "scheduler": "^0.19.1",
        "jest": "^26.0.1",
        "@prisel/jest-preset": "0.0.0"
    },
    "jest": {
        "preset": "@prisel/jest-preset"
    }
}
