{
    "name": "@prisel/react",
    "version": "0.1.5",
    "description": "Server debugger made with React",
    "author": "Yiou Chen <chen.yiou1992@gmail.com>",
    "homepage": "https://github.com/SeawolvesAtCali/prisel/tree/master/packages/template#readme",
    "license": "MIT",
    "main": "./lib/index.cjs.js",
    "module": "./lib/index.esm.js",
    "types": "./lib/index.d.ts",
    "files": [
        "lib"
    ],
    "directories": {
        "lib": "./",
        "test": "__test__"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SeawolvesAtCali/prisel.git"
    },
    "scripts": {
