{
    "name": "prisel-mono",
    "version": "0.0.0",
    "private": true,
    "description": "Online prisel game",
    "scripts": {
        "start:server": "lerna run start --scope @prisel/server --stream",
        "start:client": "lerna run start --scope @prisel/client --stream",
        "start:tic-tac-toe-server": "lerna run start --scope @prisel/tic-tac-toe --stream",
        "start:tic-tac-toe-client": "lerna run start --scope @prisel/tic-tac-toe-client --stream",
        "start:big2": "lerna run start --scope @prisel/big2 --stream",
        "clean": "lerna clean --yes && lerna exec -- rm -rf lib && rm -rf node_modules",
        "postinstall": "npm run build",
        "publish": "lerna publish",
        "build": "lerna run build --stream",
        "deploy:server": "node packages/tic-tac-toe/lib/index.js",
        "test": "lerna run --no-bail test --ignore @prisel/e2e --stream",
        "automation": "lerna run test --scope @prisel/e2e --stream",
        "storybook": "lerna run storybook --stream",
        "lint": "lerna run lint",
        "fix": "lerna run fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SeawolvesAtCali/prisel.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SeawolvesAtCali/prisel/issues"
    },
    "homepage": "https://github.com/SeawolvesAtCali/prisel#readme",
    "eslintConfig": {
        "extends": [
            "airbnb-base",
            "plugin:prettier/recommended"
        ],
        "parser": "babel-eslint",
        "rules": {
            "no-unused-vars": [
                "error",
                {
                    "vars": "local",
                    "args": "none",
                    "varsIgnorePattern": "debug"
                }
            ],
            "class-methods-use-this": "off",
            "no-unused-expressions": [
                "error",
                {
                    "allowShortCircuit": true
                }
            ],
            "no-plusplus": "off",
            "no-console": "off",
            "no-case-declarations": "off",
            "global-require": "off",
            "import/no-dynamic-require": "off",
            "no-param-reassign": "off",
            "import/no-extraneous-dependencies": [
                "error",
                {
                    "devDependencies": true
                }
            ]
        },
        "env": {
            "jest": true
        }
    },
    "engines": {
        "node": "^10.x"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "proseWrap": "always"
    },
    "dependencies": {
        "@prisel/big2": "file:packages/big2",
        "@prisel/client": "file:packages/client",
        "@prisel/common": "file:packages/common",
        "@prisel/dashboard": "file:packages/dashboard",
        "@prisel/e2e": "file:packages/e2e",
        "@prisel/server": "file:packages/server",
        "@prisel/template": "file:packages/template",
        "@prisel/tic-tac-toe": "file:packages/tic-tac-toe",
        "@prisel/tic-tac-toe-client": "file:packages/tic-tac-toe-client"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^4.1.4",
        "@storybook/react": "^4.1.4",
        "@types/debug": "0.0.30",
        "@types/jest": "^23.3.1",
        "@types/lodash": "^4.14.116",
        "@types/react": "^16.7.17",
        "@types/react-dom": "^16.0.11",
        "antd": "^3.11.2",
        "babel-eslint": "^8.2.6",
        "babel-jest": "^23.4.2",
        "babel-loader": "^8.0.4",
        "eslint": "4.19.1",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "2.12.0",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.12.0",
        "jest": "^23.6.0",
        "jest-dev-server": "^3.7.0",
        "lerna": "^3.6.0",
        "mock-socket": "^8.0.5",
        "nodemon": "^1.18.9",
        "parcel-bundler": "^1.10.3",
        "prettier": "^1.12.1",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "ts-jest": "^23.1.4",
        "ts-node": "^7.0.1",
        "tslint": "~5.11.0",
        "tslint-config-prettier": "~1.15.0",
        "typescript": "^3.0.3"
    }
}
