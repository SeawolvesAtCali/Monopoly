{
    "name": "prisel-mono",
    "version": "0.0.0",
    "private": true,
    "description": "Online prisel game",
    "scripts": {
        "bootstrap": "lerna bootstrap --hoist",
        "start:server": "lerna run start --scope @prisel/server --stream",
        "start:client": "lerna run start --scope @prisel/react --stream",
        "start:tic-tac-toe-server": "lerna run start --scope @prisel/tic-tac-toe --stream",
        "start:tic-tac-toe-client": "lerna run start --scope @prisel/tic-tac-toe-client --stream",
        "start:monopoly-server": "lerna run start --scope @prisel/monopoly --stream",
        "start:debugger": "lerna run start --scope @prisel/react --stream",
        "start:map-creator": "lerna run start --scope @prisel/map-creator --stream",
        "clean": "lerna clean --yes && lerna exec -- rm -rf lib && rm -rf node_modules",
        "postinstall": "npm run build",
        "publish": "lerna publish",
        "prepublishOnly": "npm ci && npm run build && npm run lint",
        "build": "lerna run build --stream",
        "deploy:server": "env DEBUG=debug node packages/tic-tac-toe/lib/index.js",
        "test": "lerna run --no-bail test --ignore @prisel/e2e --stream",
        "automation": "lerna run test --scope @prisel/e2e --stream",
        "storybook": "lerna run storybook --stream",
        "lint": "lerna run lint",
        "fix": "lerna run fix",
        "docgen": "lerna run docgen"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SeawolvesAtCali/prisel.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SeawolvesAtCali/prisel/issues"
    },
    "homepage": "https://github.com/SeawolvesAtCali/prisel#readme",
    "eslintConfig": {
        "extends": [
            "airbnb-base",
            "plugin:prettier/recommended"
        ],
        "parser": "babel-eslint",
        "rules": {
            "no-unused-vars": [
                "error",
                {
                    "vars": "local",
                    "args": "none",
                    "varsIgnorePattern": "debug"
                }
            ],
            "class-methods-use-this": "off",
            "no-unused-expressions": [
                "error",
                {
                    "allowShortCircuit": true
                }
            ],
            "no-plusplus": "off",
            "no-console": "off",
            "no-case-declarations": "off",
            "global-require": "off",
            "import/no-dynamic-require": "off",
            "no-param-reassign": "off",
            "import/no-extraneous-dependencies": [
                "error",
                {
                    "devDependencies": true
                }
            ]
        },
        "env": {
            "jest": true
        }
    },
    "engines": {
        "node": "^10.x"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "proseWrap": "always"
    },
    "dependencies": {
        "@prisel/client": "file:packages/client",
        "@prisel/common": "file:packages/common",
        "@prisel/e2e": "file:packages/e2e",
        "@prisel/map-creator": "file:packages/map-creator",
        "@prisel/monopoly": "file:packages/monopoly",
        "@prisel/monopoly-client": "file:packages/monopoly-client",
        "@prisel/react": "file:packages/react",
        "@prisel/server": "file:packages/server",
        "@prisel/template": "file:packages/template",
        "@prisel/tic-tac-toe": "file:packages/tic-tac-toe",
        "@prisel/tic-tac-toe-client": "file:packages/tic-tac-toe-client"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^4.1.18",
        "@storybook/react": "^4.1.18",
        "@types/debug": "^4.1.4",
        "@types/jest": "^24.0.15",
        "@types/lodash": "^4.14.134",
        "@types/node": "^13.9.3",
        "@types/prismjs": "^1.16.0",
        "@types/react": "^16.8.22",
        "@types/react-dom": "^16.8.4",
        "antd": "^3.19.7",
        "autoprefixer": "^9.6.1",
        "babel-eslint": "^8.2.6",
        "babel-jest": "^23.4.2",
        "babel-loader": "^8.0.6",
        "eslint": "4.19.1",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "2.12.0",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.13.0",
        "jest": "^24.8.0",
        "jest-dev-server": "^4.2.0",
        "lerna": "^3.15.0",
        "mock-socket": "^9.0.0",
        "parcel-bundler": "^1.12.3",
        "postcss-modules": "^1.4.1",
        "prettier": "^1.18.2",
        "prismjs": "^1.17.1",
        "react": "^16.8.1",
        "react-dom": "^16.8.1",
        "react-simple-code-editor": "^0.9.14",
        "rollup": "^1.16.2",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-dts": "^1.3.0",
        "rollup-plugin-node-resolve": "^4.2.4",
        "rollup-plugin-typescript2": "^0.21.2",
        "ts-jest": "^23.1.4",
        "ts-node": "^7.0.1",
        "tslib": "^1.10.0",
        "tslint": "~5.11.0",
        "tslint-config-prettier": "~1.15.0",
        "typedoc": "^0.14.2",
        "typedoc-plugin-markdown": "^1.2.1",
        "typescript": "^3.8.3"
    }
}